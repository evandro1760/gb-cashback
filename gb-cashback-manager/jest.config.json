{
    "transform": {
        "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
        "lcov",
        "text"
    ],
    "testEnvironment": "node",
    "verbose": true,
    "moduleNameMapper": {
        "@src(.*)$": "<rootDir>/src/$1"
    }
}